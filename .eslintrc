{
  "parser": "@typescript-eslint/parser",
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "jest": true
  },
  "extends": [
    "airbnb",
    "prettier"
  ],
  "plugins": [
    "prettier",
    "eslint-plugin-import"
  ],
  "no-unused-vars": "warn",
  "prettier/prettier": [
    "error"
  ],
  "import/no-extraneous-dependencies": 0,
  "react/jsx-filename-extension": 0,
  "no-param-reassign": 0,
  "@typescript-eslint/ban-ts-comment": "off",
  "no-return-assign": "off",
  "import/no-unresolved": "off"
}